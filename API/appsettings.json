{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "ConnectionStrings": {
    "connectionString": "CONNECTION_STRING"
  },

  "ApplicationInsights": {
    "connectionString": "APPLICATION_INSIGHTS_KEY"
  },

  "JwtApplicationConfig": {
    "key": "JWT_APPLICATION_KEY",
    "issuer": "JWT_APPLICATION_ISUSER",
    "audience": "ApplicationToken",
    "expiresInMinutes": "JWT_APPLICATION_EXPIRES_MINUTES"
  },

  "JwtPasswordConfig": {
    "key": "JWT_PASSWORD_KEY",
    "issuer": "JWT_PASSWORD_ISUSER",
    "audience": "PasswordToken",
    "expiresInMinutes": "JWT_PASSWORD_EXPIRES_MINUTES"
  },

  "EmailConfig": {
    "apiKey": "SENDGRID_KEY",
    "forgotPasswordId": "SENDGRID_FORGOT_PASSWORD_ID",
    "firstAccessId": "SENDGRID_FIRST_ACCESS_ID",
    "urlFront": "URL_FRONT",
    "fromName": "FROM_NAME",
    "fromEmail": "FORM_EMAIL"
  },

  "FileConfig": {
    "BucketName": "FILE_BUCKET_NAME",
    "BucketUrl": "FILE_BUCKET_URL",
    "ServiceURL": "FILE_SERVER_URL",
    "AccessKeyId": "FILE_KEY_ID",
    "SecretKey": "FILE_SECRET_KEY"
  },

  "AllowedHosts": "*"
}
